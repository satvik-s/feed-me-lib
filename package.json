{
  "name": "@satvik-s/feed-me-lib",
  "version": "4.2.1",
  "description": "feed me project config",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
